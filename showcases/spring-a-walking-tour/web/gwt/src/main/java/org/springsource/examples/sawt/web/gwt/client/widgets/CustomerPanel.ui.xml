<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder
        xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:g='urn:import:com.google.gwt.user.client.ui'
        >

    <g:HTMLPanel>
        <DIV>
            <g:Label ui:field="customerIdLabel"/> <g:TextBox ui:field="customerId"/> <g:Button ui:field="searchCustomerButton"/>
        </DIV>
        <div>
            <g:Label ui:field="firstNameLabel"/> <g:TextBox ui:field="firstName"/>
        </div>
        <DIV>
            <g:Label ui:field="lastNameLabel"/> <g:TextBox ui:field="lastName"/>
        </DIV>
        <DIV>
            <g:Button ui:field="updateButton"/> <g:Button ui:field="cancelButton"/>
        </DIV>



    </g:HTMLPanel>

</ui:UiBinder>